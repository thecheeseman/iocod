/*******************************************************************************
 * @file config.h
 * @brief This file is autogenerated from config.h.in during 
 * the CMake configuration stage. 
 *
 * @note If you need to make changes edit the original file NOT THIS FILE.
 ******************************************************************************/
#ifndef CONFIG_H
#define CONFIG_H

/*******************************************************************************
 * iocod variables
 ******************************************************************************/

#define IC_VERSION_MAJOR    @iocod_VERSION_MAJOR@
#define IC_VERSION_MINOR    @iocod_VERSION_MINOR@
#define IC_VERSION_PATCH    @iocod_VERSION_PATCH@

/*******************************************************************************
 * platform variables
 ******************************************************************************/

/* defined to 1 if the platform is 64-bit */
#cmakedefine IC_CMAKE_PLATFORM_64BIT 1

/* architecture string */
#define IC_CMAKE_PLATFORM_ARCH "@iocod_ARCH@"

/* operating system string */
#define IC_CMAKE_PLATFORM_OS "@iocod_OS@"

/* defined to 1 if the system is Linux */
#cmakedefine IC_CMAKE_PLATFORM_LINUX 1

/* defined to 1 if the system is MacOS */
#cmakedefine IC_CMAKE_PLATFORM_MACOS 1

/* defined to 1 if the system is Windows */
#cmakedefine IC_CMAKE_PLATFORM_WINDOWS 1

/* string containing 'msvc', 'mingw', 'gnu', 'clang', etc */
#define IC_CMAKE_PLATFORM_COMPILER "@iocod_COMPILER@"

/* defined to 1 if the system is building with MSVC */
#cmakedefine IC_CMAKE_PLATFORM_MSVC 1

/* defined to 1 if the system is building with MinGW */
#cmakedefine IC_CMAKE_PLATFORM_MINGW 1

/* defined to 1 if the system is building with GCC */
#cmakedefine IC_CMAKE_PLATFORM_GCC 1

/* defined to 1 if the system is building with Clang */
#cmakedefine IC_CMAKE_PLATFORM_CLANG 1

/* compiler version */
#define IC_CMAKE_PLATFORM_COMPILER_VER "@iocod_COMPILER_VER@"

/*******************************************************************************
 * library/function variables
 ******************************************************************************/

/* defined to 1 if the system provides `strdup()` */
#cmakedefine HAVE_STRDUP 1

/* defined to 1 if the system provides `strndup()` */
#cmakedefine HAVE_STRNDUP 1

/* defined to 1 if the system provides `strlwr()` */
#cmakedefine HAVE_STRLWR 1

/* defined to 1 if the system provides `strupr()` */
#cmakedefine HAVE_STRUPR 1

/* defined to 1 if the system provides `<strings.>` */
#cmakedefine HAVE_STRINGS_H 1

/* defined to 1 if the system provides `strcasecmp()` */
#cmakedefine HAVE_STRCASECMP 1

/* defined to 1 if the system provides `strncasecmp()` */
#cmakedefine HAVE_STRNCASECMP 1

#endif /* CONFIG_H */
